{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-trailing-spaces": [2, { "skipBlankLines": true }],
    "no-console": 0,
    "indent": ["error", 4],
    "quotes": ["error", "single"],
    "linebreak-style": ["error", "windows"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
